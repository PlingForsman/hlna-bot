  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/kennyhml/hlna-bot/issues"
  },
  "homepage": "https://github.com/kennyhml/hlna-bot#readme",
  "devDependencies": {
    "@types/node": "^24.0.5",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "discord.js": "^14.21.0"
  }
}